<!DOCTYPE html>
<html>
  <head>
    <title>StockFlow</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <script type="text/javascript">
      window.loginImage = "<%= image_url("login_image.png") %>"
      window.homePageGif = "<%= image_url("home_page_gif.gif") %>"
      window.rocketKitty = "<%= image_url("rocket_kitty.png") %>"
      window.stockGraph = "<%= image_url("stock_options.png") %>"
      window.etfs = "<%= image_url("etfs.png") %>"
      window.stockOptions = "<%= image_url("stock_options.png") %>"
      window.cryptoCurr = "<%= image_url("crypto.png") %>"
      window.singlePhone = "<%= image_url("single_phone_pic.png") %>"
      window.darkMultiPhone = "<%= image_url("dark_multi_phone.png") %>"
      window.browsingPhone = "<%= image_url("browsing_phone.png") %>"
      window.eagleLogo = "<%= image_url("eagle_logo.png") %>"
      window.stockflowLogo = "<%= image_url("stock_flow_logo.png") %>"
      window.signupGIF = "<%= image_url("signup_gif.gif") %>"
    </script>

    

    <link href="https://fonts.googleapis.com/css?family=Fira+Sans:400,600,700,800,900&display=swap" rel="stylesheet">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
